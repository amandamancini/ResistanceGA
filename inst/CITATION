citHeader("To cite ResistanceGA in publications use:")

citEntry(entry = "Article",
         title = "Ecological resistance surfaces predict fine-scale genetic differentiation in a terrestrial woodland salamander",
         author = personList(as.person("William E. Peterman"),as.person("Grant M. Connette"),as.person("Raymond D. Semlitsch"),as.person("Lori S. Eggert")),
         journal = "Molecular Ecology",
         year   = "2014",
         volume = "23",
         number = "10",
         pages = "2402-2413",
         url    = "http://onlinelibrary.wiley.com/doi/10.1111/mec.12747/abstract",

 textVersion  =
  paste("Peterman, W.E., G.M. Connette, R.D. Semlitsch, and L.S. Eggert. 2014.",
        "Ecological resistance surfaces predict fine-scale genetic differentiation in a terrestrial woodland salamander.",
        "Molecular Ecology, 23:2402-2414.")
)

citEntry(entry = "Article",
         title = "ResistanceGA: An R package for the optimization of resistance surfaces using genetic algorithms",
         author = "William E. Peterman",
         journal = "Methods in Ecology and Evolution",
         year   = "2018",
         volume = "",
         number = "",
         pages = "",
         doi = "10.1111/2041-210X.12984",
         url    = "https://besjournals.onlinelibrary.wiley.com/doi/abs/10.1111/2041-210X.12984",
         textVersion  =
paste("Peterman, W.E. 2018.",
      "ResistanceGA: An R package for the optimization of resistance surfaces using genetic algorithms.",
      "Methods in Ecology and Evolution doi:10.1111/2041-210X.12984.")
)